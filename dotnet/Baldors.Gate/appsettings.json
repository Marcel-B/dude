{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Urls": "http://*:80;https://*:443",
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "/{**catch-all}",
          "Hosts": [
            "marcelbenders.com"
          ]
        }
      },
      "route2": {
        "ClusterId": "cluster2",
        "Match": {
          "Path": "/{**catch-all}",
          "Hosts": [
            "idsrv.marcelbenders.com"
          ]
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "destination1": {
            "Address": "idsrv:6070"
          }
        }
      },
      "cluster2": {
        "Destinations": {
          "destination1": {
            "Address": "devit-host:8085"
          }
        }
      }
    }
  },
  "LettuceEncrypt": {
    "AcceptTermsOfService": true,
    "DomainNames": [
      "marcelbenders.com"
    ],
    "EmailAddress": "info@marcelbenders.com"
  }
}
